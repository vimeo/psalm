{
    "name": "mattb/psalm",
    "description": "A PHP code inspection utility written in PHP",
    "keywords": ["php", "code", "inspection"],
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Matthew Brown"
        }
    ],
    "require": {
        "php": ">=5.4",
        "nikic/PHP-Parser": "2.1.0"
    },
    "bin": ["bin/psalm"],
    "autoload": {
        "psr-4": {
            "Psalm\\": "src/Psalm"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Psalm\\Tests": "tests"
        }
    }
}
